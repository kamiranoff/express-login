{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "rules": {
    "max-len": [
      2,
      160,
      2,
      {
        "ignoreUrls": true
      }
    ],
    "no-underscore-dangle": 0,
    "func-names": [
      "error",
      "as-needed"
    ],
    "space-before-function-paren": 0
  }
}